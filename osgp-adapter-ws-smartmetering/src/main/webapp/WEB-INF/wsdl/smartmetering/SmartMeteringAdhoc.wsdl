    <wsdl:portType name="SmartmeteradhocPort">
        <wsdl:operation name="SynchronizeTime">
            <wsdl:input message="tns:SynchronizeTimeRequest" name="SynchronizeTimeRequest" />
            <wsdl:output message="tns:SynchronizeTimeAsyncResponse" name="SynchronizeTimeAsyncResponse" />
        </wsdl:operation>
        
        <wsdl:operation name="GetSynchronizeTimeResponse">
            <wsdl:input message="tns:SynchronizeTimeAsyncRequest" name="SynchronizeTimeAsyncRequest" />
            <wsdl:output message="tns:SynchronizeTimeResponse" name="SynchronizeTimeResponse" />
        </wsdl:operation>        

        <wsdl:operation name="SendWakeupSMS">
            <wsdl:input message="tns:SendWakeupSMSRequest" name="SendWakeupSMSRequest" />
            <wsdl:output message="tns:SendWakeupSMSAsyncResponse" name="SendWakeupSMSAsyncResponse" />

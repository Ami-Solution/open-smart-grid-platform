    </wsdl:message>
    <wsdl:message name="SynchronizeTimeAsyncResponse">
        <wsdl:part element="smadhoc:SynchronizeTimeAsyncResponse" name="SynchronizeTimeAsyncResponse">
        </wsdl:part>
    </wsdl:message>
    <wsdl:message name="SynchronizeTimeAsyncRequest">
        <wsdl:part element="smadhoc:SynchronizeTimeAsyncRequest" name="SynchronizeTimeAsyncRequest">
        </wsdl:part>
    </wsdl:message>
    <wsdl:message name="SynchronizeTimeResponse">
        <wsdl:part element="smadhoc:SynchronizeTimeResponse" name="SynchronizeTimeResponse">
        </wsdl:part>
    </wsdl:message>
    
    <!-- SEND WAKEUP SMS -->
    
    <wsdl:message name="SendWakeupSMSRequest">
